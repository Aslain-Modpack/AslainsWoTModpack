  <!-- Copy the nuget dlls to the project's dll directory by overiding a predefined target -->
  <!-- https://docs.microsoft.com/en-us/visualstudio/msbuild/how-to-extend-the-visual-studio-build-process?view=vs-2019 -->
  <Target Name="BeforeBuild">
    <!-- Item group of only dlls from nuget -->
    <ItemGroup>
      <Filtered Include="%(Reference.HintPath)" Condition=" '%(Reference.HintPath)' != '' " />
    </ItemGroup>
    <Message Text="Copying dlls" />
    <!-- Create a property of the dll output directory -->
    <PropertyGroup>
      <DllOutputDir>Resources\DLL</DllOutputDir>
    </PropertyGroup>
    <!-- <Message Text="@(Filtered)" /> -->
    <!-- Get the dlls to include by determining if they have a hint path. Any with it are nuget dlls to include-->
    <!-- https://docs.microsoft.com/en-us/visualstudio/msbuild/makedir-task?view=vs-2019 -->
    <MakeDir Directories="$(DllOutputDir)" />
    <!-- https://docs.microsoft.com/en-us/visualstudio/msbuild/copy-task?view=vs-2019 -->
    <Copy SourceFiles="@(Filtered)" DestinationFolder="$(DllOutputDir)" />
  </Target>
  <Target Name="ZipImageLibs" AfterTargets="BeforeBuild">
    <ItemGroup>
      <!-- relative path is project root. Get the teximp path -->
      <!-- Get all items that have a hint path (nuget only) -->
      <NugetItems Include="%(Reference.HintPath)" Condition=" '%(Reference.HintPath)' != '' " />
      <!-- Get only TeximpNet item -->
      <!-- example return: "TeximpNugetRoot__: C:\Users\Willster419\Tanks Stuff\RelhaxModpack\RelhaxModpack\packages\TeximpNet.1.4.2\lib\net40\TeximpNet.dll" -->
      <TeximpNugetFullpath Include="%(NugetItems.Fullpath)" Condition=" '%(Filename)%(Extension)' == 'TeximpNet.dll' " />
    </ItemGroup>
    <PropertyGroup>
      <!-- get the vector of 1 (item) and convert it to a scalar -->
      <!-- example return: TeximpNugetPathProp: "C:\Users\Willster419\Tanks Stuff\RelhaxModpack\RelhaxModpack\packages\TeximpNet.1.4.2\lib\net40\TeximpNet.dll" -->
      <TeximpNugetPathProp>%(TeximpNugetFullpath.Identity)</TeximpNugetPathProp>
      <!-- go up in the paths until we're at the teximpnet nuget root -->
      <TeximpNugetPathUp1>$([System.IO.Path]::GetDirectoryName($(TeximpNugetPathProp)))</TeximpNugetPathUp1>
      <TeximpNugetPathUp2>$([System.IO.Path]::GetDirectoryName($(TeximpNugetPathUp1)))</TeximpNugetPathUp2>
      <TeximpNugetPathUp3>$([System.IO.Path]::GetDirectoryName($(TeximpNugetPathUp2)))</TeximpNugetPathUp3>
    </PropertyGroup>
    <ItemGroup>
      <!-- define the 32 and 64bit freeimage and nvtt libraries -->
      <FreeImage32 Include="$(TeximpNugetPathUp3)\runtimes\win-x86\native\FreeImage.dll" />
      <FreeImage64 Include="$(TeximpNugetPathUp3)\runtimes\win-x64\native\FreeImage.dll" />
      <NVTT32 Include="$(TeximpNugetPathUp3)\runtimes\win-x86\native\nvtt.dll" />
      <NVTT64 Include="$(TeximpNugetPathUp3)\runtimes\win-x64\native\nvtt.dll" />
    </ItemGroup>
    <PropertyGroup>
      <!-- define the paths of the 32 and 64bit freeimage and nvtt libraries inside teximpnet nuget path -->
      <FreeImage32Path>$([System.IO.Path]::GetDirectoryName(%(FreeImage32.Identity)))</FreeImage32Path>
      <FreeImage64Path>$([System.IO.Path]::GetDirectoryName(%(FreeImage64.Identity)))</FreeImage64Path>
      <NVTT32Path>$([System.IO.Path]::GetDirectoryName(%(NVTT32.Identity)))</NVTT32Path>
      <NVTT64Path>$([System.IO.Path]::GetDirectoryName(%(NVTT64.Identity)))</NVTT64Path>
    </PropertyGroup>
    <PropertyGroup>
      <!-- define the destination copy paths for the freeimage and nvtt dlls -->
      <FreeImage32CopyPath>$(DllOutputDir)\FreeImage32.dll</FreeImage32CopyPath>
      <FreeImage64CopyPath>$(DllOutputDir)\FreeImage64.dll</FreeImage64CopyPath>
      <NVTT32CopyPath>$(DllOutputDir)\nvtt32.dll</NVTT32CopyPath>
      <NVTT64CopyPath>$(DllOutputDir)\nvtt64.dll</NVTT64CopyPath>
    </PropertyGroup>
    <Message Text="Copying FreeImage and NVTT" />
    <!-- 
    <Message Text="TeximpNugetFullpath: @(TeximpNugetFullpath)" />
    <Message Text="TeximpNugetPathProp: $(TeximpNugetPathProp)" />
    <Message Text="TeximpNugetPathUp1: $(TeximpNugetPathUp1)" />
    <Message Text="TeximpNugetPathUp2: $(TeximpNugetPathUp2)" />
    <Message Text="TeximpNugetPathUp3: $(TeximpNugetPathUp3)" />
    <Message Text="DllOutputDir: $(DllOutputDir)" />
    <Message Text="FreeImage32: @(FreeImage32)" />
    <Message Text="FreeImage64: @(FreeImage64)" />
    <Message Text="NVTT32: @(NVTT32)" />
    <Message Text="NVTT64: @(NVTT64)" />
    <Message Text="FreeImage32Path: $(FreeImage32Path)" />
    <Message Text="FreeImage64Path: $(FreeImage64Path)" />
    <Message Text="NVTT32Path: $(NVTT32Path)" />
    <Message Text="NVTT64Path: $(NVTT64Path)" />
    -->
    <!-- Copy the files out to the DLL directory so we can rename them -->
    <Copy SourceFiles="@(FreeImage32)" DestinationFiles="$(FreeImage32CopyPath)" />
    <Copy SourceFiles="@(FreeImage64)" DestinationFiles="$(FreeImage64CopyPath)" />
    <Copy SourceFiles="@(NVTT32)" DestinationFiles="$(NVTT32CopyPath)" />
    <Copy SourceFiles="@(NVTT64)" DestinationFiles="$(NVTT64CopyPath)" />
    <!-- Create the zip files -->
    <MSBuild.Community.Tasks.Zip Files="$(FreeImage32CopyPath)" ZipFileName="$(DllOutputDir)\FreeImage32.zip" WorkingDirectory="$(DllOutputDir)" />
    <MSBuild.Community.Tasks.Zip Files="$(FreeImage64CopyPath)" ZipFileName="$(DllOutputDir)\FreeImage64.zip" WorkingDirectory="$(DllOutputDir)" />
    <MSBuild.Community.Tasks.Zip Files="$(NVTT32CopyPath)" ZipFileName="$(DllOutputDir)\nvtt32.zip" WorkingDirectory="$(DllOutputDir)" />
    <MSBuild.Community.Tasks.Zip Files="$(NVTT64CopyPath)" ZipFileName="$(DllOutputDir)\nvtt64.zip" WorkingDirectory="$(DllOutputDir)" />
    <!-- Delete the dlls after zipping -->
    <!-- https://docs.microsoft.com/en-us/visualstudio/msbuild/delete-task?view=vs-2019 -->
    <Delete Files="$(FreeImage32CopyPath)" />
    <Delete Files="$(FreeImage64CopyPath)" />
    <Delete Files="$(NVTT32CopyPath)" />
    <Delete Files="$(NVTT64CopyPath)" />
  </Target>
